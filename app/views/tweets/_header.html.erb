<div class="col-md-5 tweets-index-header-left">
  <div class="col-md-3 tweets-header-tab">
    <%= link_to "Home", authenticated_root_path, class: "tweets-header-link" %>
  </div>
  <div class="col-md-3">
    <%= link_to "Moments", "#", class: "tweets-header-link" %>
  </div>
  <div class="col-md-3">
    <%= link_to "Notifications", "#", class: "tweets-header-link" %>
  </div>
  <div class="col-md-3">
    <%= link_to "Messages", "#", class: "tweets-header-link" %>
  </div>
</div>

<div class="col-md-2">
    <%= link_to (image_tag "logo-blue.png", class: "logo"), authenticated_root_path %>
</div>

<div class="col-md-5">
  <div class="col-md-7 col-md-offset-1">
    <input class="search" placeholder="Search Twitter" />
  </div>
  <div class="col-md-1 tweets-index-header-avatar">
    <%= image_tag "avatar.png", class: "user-meta-container-avatar" %>
  </div>
  <div class="col-md-2 tweet-button">
    Tweet
  </div>
</div>
